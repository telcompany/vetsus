<div class="modal fade" id="vetModal">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 id="lblTitleModal" class="modal-title">Nuevo doctor</h5>
                <button type="button" class="close" onclick="closeModal()">
                    <span>&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form id="vetModalForm">
                    <input type="hidden" id="Id" name="Id" value="">
                    <div class="form-group">
                        <label class="text-black font-w500" for="Firstname">Nombres</label>
                        <input id="Firstname" name="Firstname" type="text" class="form-control input-rounded">
                    </div>
                    <div class="form-group">
                        <label class="text-black font-w500" for="Lastname">Apellidos</label>
                        <input id="Lastname" name="Lastname" type="text" class="form-control input-rounded">
                    </div>
                    <div class="form-group">
                        <label class="text-black font-w500" for="Phone">Teléfono</label>
                        <input id="Phone" name="Phone" type="text" class="form-control input-rounded">
                    </div>
                    <div class="form-group">
                        <button type="button" class="btn btn-primary" onclick="addOrEditVetAction()">Guardar</button>
                        <button type="button" class="btn btn-light" onclick="closeModal()">Cerrar</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>